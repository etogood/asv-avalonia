<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:geoMap="clr-namespace:Asv.Avalonia.GeoMap"
             xmlns:avalonia="clr-namespace:Asv.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:CompileBindings="True"
             x:DataType="avalonia:PositionDialogViewModel"
             x:Class="Asv.Avalonia.GeoMap.PositionDialogView">
    
    <Design.DataContext>
        <avalonia:PositionDialogViewModel />
    </Design.DataContext>

    <Panel>
        <StackPanel Spacing="10" Margin="10">
            <TextBlock Text = "Path to nuget package" FontSize="18" FontWeight="Bold"/>
            <TextBox Watermark="C://Documents/package.nupkg"/>
        </StackPanel>
    </Panel>
    
</UserControl>
